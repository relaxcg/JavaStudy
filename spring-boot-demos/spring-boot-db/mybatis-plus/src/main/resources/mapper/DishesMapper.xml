<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.relaxcg.mp.mapper.DishesMapper">
    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.relaxcg.mp.entity.DishesEntity">
        <result property="id" column="id" jdbcType="VARCHAR"/>
        <result property="info" column="info" jdbcType="VARCHAR" typeHandler="com.relaxcg.mp.config.JsonbTypeHandler"/>
    </resultMap>
    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, info
    </sql>
</mapper>
